{% extends 'base_client.html.twig' %}

{% block meta %}
{{parent()}}
<meta http-equiv="cache-control" content="max-age=0" />
<meta http-equiv="cache-control" content="no-cache" />
<meta http-equiv="expires" content="0" />
<meta http-equiv="expires" content="Tue, 01 Jan 1980 1:00:00 GMT" />
<meta http-equiv="pragma" content="no-cache" />
{% endblock %}

{% block body %}
    
{% block menu %}
    {{ parent() }}
{% endblock %}

    <div class="container content">

        <div class="row content">
            <table class="table table-striped table-bordered">

            <tr>
                <td colspan="2">
                    <button class="btn btn-xs btn-primary submit">
                        Enregistrer
                    </button>
                </td>
            </tr>
            <tr>
                <td colspan="3" style="margin-top:10px;">
                    <strong>Modification d'un matricule</strong>
                </td>
            </tr>
            <tr>
                <td><strong>prestataireRessourceId</strong></td>
                <td>
                    <input type="text" value="{{prestataireRessourceId}}" class="prestataireRessourceId" />
                </td>
            </tr>
            <tr>
                <td><strong>Matricule chez OT/EC</strong></td>
                <td>
                    <input type="text" value="{{matricule_OT}}" class="matricule_OT" />
                </td>
            </tr>
            <input type="hidden" value="{{id}}" class="id" />
            
            </table>
        </div><!-- row navigation-->
    </div><!-- container navigation-->

{% endblock %}

{% block javascripts %}
    {{parent()}}
    <script type="text/javascript" src="{{ asset('js/client/matricule.js') }}"></script>
{% endblock %}